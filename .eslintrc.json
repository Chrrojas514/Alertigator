{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style" : "off",
        "no-use-before-define" : "off",
        "no-restricted-syntax" : "off",
        "no-await-in-loop" : "off",
        "global-require" : "off",
        "no-" : "off"
    }
}
